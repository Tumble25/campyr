<div class="dashboard-container">
  <h4 class="m-3">My campsites</h4>
  <div class="grid-container">

      <!-- each  -->
    <% campsites.each do |campsites| %>
    <%= link_to(campsite_path(campsites)) do %>
      <div class="campsite-card ">
         <img src=<%= cl_image_path campsites.photo%>>
        <div class="campsite-card-infos d-flex justify-content-center">
          <div>
            <h2><%= campsites.name %></h2>
          </div>
        </div>
      </div>
         <% end %>
         <% end %>
      <!-- end of each -->

  </div>
</div>
